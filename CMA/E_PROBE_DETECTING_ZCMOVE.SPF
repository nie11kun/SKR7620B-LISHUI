PROC E_PROBE_DETECTING_ZCMOVE(REAL Z_VALUE,REAL C_ANG,REAL SPEED) DISPLOF
;***********程序功能**********
;*腰型孔检测的ZC动作
;
;
;
;****************************

IF TOOL_SET[55]==0;
    IF TOOL_SET[51]==0;
        MEAS=1 G91 G01 Z=Z_VALUE C=C_ANG F=SPEED
    ELSE
        MEAS=2 G91 G01 Z=Z_VALUE C=C_ANG F=SPEED
    ENDIF
ELSE
    IF TOOL_SET[52]==0;
        MEAS=1 G91 G01 Z=Z_VALUE C=C_ANG F=SPEED
    ELSE
        MEAS=2 G91 G01 Z=Z_VALUE C=C_ANG F=SPEED
    ENDIF
ENDIF

STOPRE

RET

