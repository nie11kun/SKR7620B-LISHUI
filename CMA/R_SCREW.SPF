PROC R_SCREW DISPLON
;***********程序功能**********
;*台阶主程序:
;****************************

S_PAR_INI;

IF DRESSER[6]<>0;
    GOTOF TJ_G_ENDING
ENDIF

IF TECHNOLOGY[265]+TECHNOLOGY[266]==0;
    GOTOF TJ_G_ENDING
ENDIF

M20;
M26;

WHILE(TECHNOLOGY[263]+TECHNOLOGY[264]<=TECHNOLOGY[265]+TECHNOLOGY[266])
    STOPRE
    S_CYCLE;
    IF (DRESSER[6]<>0) OR ($A_DBB[2]==1);
        GOTOF TJ_G_ENDING
    ENDIF
    S_BCYC;
	S_LINESPEED_GRIND;
ENDWHILE

TJ_G_ENDING:
S_GRINDING_END;
C_ALARM;

RET

