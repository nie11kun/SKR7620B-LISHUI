PROC E_PROBE_DETECTED_XMOVE(REAL X_VALUE,REAL SPEED) DISPLOF
;***********程序功能**********
;*开关检测到后X的动作
;
;
;****************************

IF TOOL_SET[55]==0;
    F_PROBE_A_PROBE(TOOL_SET[51])
ELSE
    F_PROBE_A_PROBE(TOOL_SET[52])
ENDIF
WHILE(TOOL_SET[54]==1);
	G91 G01 X=X_VALUE F=SPEED
	IF TOOL_SET[55]==0;
		F_PROBE_A_PROBE(TOOL_SET[51])
	ELSE
		F_PROBE_A_PROBE(TOOL_SET[52])
	ENDIF
ENDWHILE

STOPRE

RET

