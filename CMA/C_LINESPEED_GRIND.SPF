PROC C_LINESPEED_GRIND SBLOF DISPLOF
;***********程序功能**********
;*转速:
;*砂轮转速计算及执行
;****************************

DEF REAL WHEEL_RPM;

D_GRINDWHEEL_DIACURNT;

IF INI[144]==0;
    WHEEL_RPM=PROCESS[10]*60000/($PI*DRESSER[24])
ELSE
    WHEEL_RPM=PROCESS[10]
ENDIF

IF WHEEL_RPM>INI[150];
    WHEEL_RPM=INI[150]
ENDIF

F_GRINDWHEEL_RUN(WHEEL_RPM);

RET

